    "lint-check": "npm run check-git-hooks && eslint --cache --report-unused-disable-directives . && prettier --check .",
    "prepare": "cd .. && husky"
  },
  "dependencies": {
    "@vnedyalk0v/react19-simple-maps": "^1.2.1",
    "autoprefixer": "^10.4.23",
    "email-validator": "^2.0.4",
    "firebase": "^12.8.0",
    "next": "^16.1.6",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-simple-maps": "^3.0.6",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5"
  }
